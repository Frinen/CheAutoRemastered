@inherits IndexModel
@page "/"

@if (LoggedIn)
{
    <h1>Hello, world! </h1>
    <p>Welcome to your new app.</p>
}
else
{
    <input type="text" @bind-value="Email" />
    <p />
    <input type="text" @bind-value="Password" />
    <p />
    <button @onclick="Login">Login</button>
    <p>or</p> 
    <button @onclick="Register">Register</button>
}




